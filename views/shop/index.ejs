<%- include('../includes/head.ejs') %>
</head>

<body>
  <%- include('../includes/navigation') %>

  <main>
    <p>PRODUCT INDEX</p>
    <% if (products.length > 0) { %>
    <div class="card">
      <ul>
        <% products.forEach((product) => { %>
        <li><%= product.title %></li>
        <% }) %>
      </ul>
    </div>
    <% } else { %>
    <h1>No products registered</h1>
    <% } %>
  </main>
  <%- include('../includes/end') %>